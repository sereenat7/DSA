class Solution {
    public boolean isSubsequence(String s, String t) {
        int m=s.length();
        int l=t.length();
        int count=0;
        int i=0;int j=0;
        while( i<m && j<l){
             {
            if(s.charAt(i)==t.charAt(j)){
                i++;
            }
            else{
                j++;
            }
             }
        }
        if(m==i){
            return true;
        }
        else{
            return false;
        }
    }
}
