class Solution {
    public int majorityElement(int[] nums) {
        int i=1;
        int j=nums.length/2;
        int count=0;
        int maj=nums[0];
        while(i<nums.length){

            if(nums[i-1]>nums[i]){
                maj=nums[i-1];

            }
            i++;
        }

       for(int k=0;k<nums.length;k++){
        if(maj==nums[k]){
            count++;
        }
       }

       if(count>=nums.length/2){
        return maj;
       }
        return 0;
    }
}
