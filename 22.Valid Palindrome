class Solution {
    public boolean isPalindrome(String s) {
        String cleaned=s.replaceAll("[^a-zA-Z0-9]","");
        String news=cleaned.toLowerCase();
        int i=0;
        int m=news.length()-1;
        while(i<m){
            if(news.charAt(i)!=news.charAt(m)){
                return false;
            }
           i++;
            m--; 
        }
       
        return true;
       
    }
}
